/dts-v1/;
#include "mt6582.dtsi"
#include "skeleton.dtsi"
//#include "mt6323.dtsi"

/ {
  model = "Huawei Honor Holly/3C Lite (Hol-U19)";
	compatible = "huawei,holly";

	chosen {
		tick-timer = &gpt;
	};

	memory {
		#address-cells = <1>;
		#size-cells = <1>;
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	// use framebuffer set up by stock lk 
	fb0: framebuffer@beb00000 {
		compatible = "simple-framebuffer";
		reg = <0xbeb00000 (1280 * 720 * 2)>;
		width = <720>;
		height = <1280>;
		stride = <(720 * 2)>;
		format = "r5g6b5";
		status = "okay";
	};
	
